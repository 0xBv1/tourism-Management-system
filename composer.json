{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alexusmai/laravel-file-manager": "^3.0",
        "artesaos/seotools": "^1.0",
        "astrotomic/laravel-translatable": "^11.11",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "darkaonline/l5-swagger": "^8.5",
        "doctrine/dbal": "^3.6",
        "dompdf/dompdf": "^2.0",
        "google/cloud-translate": "^1.16",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.5",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "^3.1",
        "nasr/laravel-lang-translatable": "^1.0",
        "predis/predis": "^2.2",
        "romanzipp/laravel-queue-monitor": "^5.3",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-sitemap": "^6.4",
        "srmklive/paypal": "~3.0",
        "stichoza/google-translate-php": "^5.1",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2",
        "timokoerber/laravel-one-time-operations": "^1.3",
        "twilio/sdk": "^7.4",
        "unlu/laravel-api-query-builder": "^1.7",
        "yajra/laravel-datatables": "^9.0",
        "yajra/laravel-datatables-oracle": "^10.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.18",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Documentation\\": "documentation",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/admin.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php fix-carbon-deprecation.php",
            "@php fix-php84-deprecations.php"
        ],
        "post-install-cmd": [
            "@php fix-carbon-deprecation.php",
            "@php fix-php84-deprecations.php"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
